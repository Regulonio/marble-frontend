{
  "name": "ui-design-system-e2e",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ui/design-system-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "libs/ui/design-system-e2e/cypress.config.ts",
        "devServerTarget": "ui-design-system:storybook",
        "testingType": "e2e"
      },
      "configurations": {
        "ci": {
          "devServerTarget": "ui-design-system:storybook:ci"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/ui/design-system-e2e/**/*.{js,ts}"]
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsc --noEmit -p libs/ui/design-system-e2e/tsconfig.json"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["ui-design-system"]
}
