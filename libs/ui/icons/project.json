{
  "name": "ui-icons",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ui/icons/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/ui/icons",
        "tsConfig": "libs/ui/icons/tsconfig.lib.json",
        "project": "libs/ui/icons/package.json",
        "entryFile": "libs/ui/icons/src/index.ts",
        "external": [
          "react/jsx-runtime"
        ],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "swc",
        "assets": [
          {
            "glob": "libs/ui/icons/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/ui/icons/**/*.ts"
        ]
      }
    },
    "icons-gen": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node -r esbuild-register src/scripts/generate.ts",
        "cwd": "./libs/ui/icons"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsc --noEmit -p libs/ui/icons/tsconfig.lib.json"
      }
    }
  },
  "tags": [
    "type:ui",
    "scope:shared"
  ]
}
