{
  "name": "openapi-backend",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/openapi/backend/src",
  "projectType": "library",
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/openapi/backend",
        "command": "redocly preview-docs"
      }
    },
    "bundle": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/openapi/backend",
        "command": "redocly bundle -o dist"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/openapi/backend",
        "command": "redocly lint"
      }
    }
  },
  "tags": ["openapi"]
}
